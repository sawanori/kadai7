
 <div class="col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1 confirm">
<h2>この内容でつぶやく</h2>

<%= form_for(@tweet) do |f| %>
 <%= f.hidden_field :content %>
<p>本文：<%= @tweet.content %></p>
<%= f.submit "登録する", class: "btn btn-primary btn-block" %>
<%= f.submit '戻る', name:'back', class:"btn btn-primary btn-block" %>
<% end %>
</div>
